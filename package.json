{
  "name": "digitransit-ui-nightwatch",
  "version": "0.0.1",
  "description": "Runs browser tests specific for rutebanken",
  "licence": "MIT",
  "keywords": ["rutebanken", "nightwatch"],
  "bin": {
    "nightwatch": "./node_modules/nightwatch/bin/nightwatch"
  },
  "scripts": {
    "test": "nightwatch --skiptags skip",
