    "beta": "nightwatch --skiptags skip --env beta",
    "beta-smoke": "nightwatch --tag smoke --env beta"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rutebanken/digitransit-ui-nightwatch.git"
  },
